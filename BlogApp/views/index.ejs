<% include ./partials/header %>

<h1>The sweet index page</h1>

<% blogs.forEach(function(blog){ %>
	<div>
		<h2><%= blog.title %></h2>
		<img src="<%= blog.image %>">
		<span> <%= blog.create %></span>
		<p><%= blog.body %></p>
		<a href="/blogs/<%= blog._id %>">Read More</a>
	</div>
<% }) %>


<% include ./partials/footer %>